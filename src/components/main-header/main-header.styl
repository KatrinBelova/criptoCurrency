.mainHeader
	background-color #f3f9ff
	font 700 16px mainFont
	height 130px
	padding-top 35px
	padding-bottom 40px
	&__logo
		width 182px
		height 55px
	&__nav
		width 100%
		display flex
		justify-content space-between
		align-items center
	&__list
		display flex
		text-transform uppercase
		color gray
		li
			padding-right 16px
		li:last-child
			padding-right 0
	&__button
		position relative
		
	